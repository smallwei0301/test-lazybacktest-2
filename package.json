{
  "name": "test-lazybacktest",
  "version": "0.1.0",
  "private": true,
  "description": "LazyBacktest with Netlify Functions proxy",
  "scripts": {
    "build": "tailwindcss -i ./css/tailwind-input.css -o ./css/tailwind.css",
    "watch": "tailwindcss -i ./css/tailwind-input.css -o ./css/tailwind.css --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:legacy": "node tests/batch-context.test.js && node tests/batch-mapper.test.js && node tests/strategy-composer.test.js"
  },
  "dependencies": {
    "@netlify/blobs": "^6.0.0",
    "node-fetch": "^2.6.7",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/container-queries": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "autoprefixer": "^10.4.16",
    "jest": "^30.2.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.6"
  }
}
